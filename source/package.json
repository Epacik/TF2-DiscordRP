{
  "name": "tf2-discord-rpc",
  "version": "1.0.0",
  "description": "Discord RPC for Team Fortress 2",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build",
    "sass": "node-sass -o src/css/ src/sass/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Epat9/node-TF2-DiscordRP.git"
  },
  "homepage": "https://github.com/Epat9/node-TF2-DiscordRP",
  "keywords": [
    "Electron",
    "Discord",
    "RPC"
  ],
  "author": "Damian Ku≈õmierz",
  "license": "MIT",
  "dependencies": {
    "discord-rpc": "./RPC",
    "discord.js": "~11.3.2",
    "electron-settings": "^3.2.0",
    "jquery": "^3.3.1",
    "jsdom": "^11.10.0",
    "ps-node": "~0.1.6"
  },
  "devDependencies": {
    "electron": "2.0.2",
    "electron-builder": "^20.13.3",
    "node-sass": "^4.9.0"
  },
  "build": {
    "appId": "com.tf2RPC",
    "win": {
      "target": "nsis",
      "icon": "/src/img/512x512.png"
    },
    "linux": {
      "target": [
        "AppImage",
        "snap"
      ]
    }
  },
  "bugs": {
    "url": "https://github.com/Epat9/node-TF2-DiscordRP/issues"
  }
}
